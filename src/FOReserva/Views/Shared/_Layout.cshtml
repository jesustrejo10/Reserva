    <div id="preloader">
        <div class="inner">
            <span class="loader"></span>
        </div>
    </div><!-- /PRELOADER -->
    <!-- JAVASCRIPT FILES -->
    <script type="text/javascript">var plugin_path = "@Url.Content("~/Content/assets/plugins/")";</script>
    <!-- JS DATATABLES -->
